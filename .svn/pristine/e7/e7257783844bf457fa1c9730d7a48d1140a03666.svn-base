<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/tr_reporte_presencia_elemvisibilidad"
    style="@style/BodyGrilla" >

    <TextView
        android:id="@+id/tv_codigo"
        style="@style/TextoBodyGrilla"
        android:layout_width="55dp" />

    <LinearLayout
        android:layout_width="45dp"
        android:layout_height="match_parent"
        android:gravity="center_vertical|center_horizontal"
        android:paddingBottom="5dp"
        android:paddingTop="5dp" >

        <CheckBox
            android:id="@+id/ck_cuenta"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:button="@drawable/checkboxstyle" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="140dp"
        android:layout_height="match_parent"
        android:gravity="center_vertical|center_horizontal"
        android:paddingBottom="5dp"
        android:paddingTop="5dp" >

        <CheckBox
            android:id="@+id/ck_cumple"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:button="@drawable/checkboxstyle" />
    </LinearLayout>

</TableRow>