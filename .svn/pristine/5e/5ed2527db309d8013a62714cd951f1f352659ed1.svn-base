<?xml version="1.0" encoding="utf-8"?>


<resources>
	<style name="Alertas" parent="@android:style/Theme.Dialog">
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:typeface">serif</item>
        <item name="android:textSize">12sp</item>
    </style>
    
    <style name="Titulo">
<!--         #0863a5 -->
       <item name="android:textColor">#ef0094</item>
        <item name="android:typeface">sans</item>
        <item name="android:textSize">18sp</item>
        <item name="android:gravity">right|center_vertical</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">17dp</item>
        <item name="android:layout_gravity">right</item>
        <item name="android:ellipsize">end</item>
		<item name="android:singleLine">true</item>
		<item name="android:layout_width">210dp</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    
    <style name="Subtitulo">
<!--         ef0094 -->
       <item name="android:textColor">#ffffff</item>
        <item name="android:typeface">sans</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">18sp</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:singleLine">false</item>
    </style>
    
      <style name="SubtituloFiltros">
<!--         ef0094 -->
       <item name="android:textColor">#ffffff</item>
        <item name="android:typeface">sans</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">15sp</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">false</item>
    </style>

        
    
    <style name="SubtituloPV">
<!--         ef0094 -->
       <item name="android:textColor">#ffffff</item>
        <item name="android:typeface">sans</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">18sp</item>
        <item name="android:gravity">center_horizontal</item>
       
       
        
        
    </style>
        
    <style name="PrimerLineaListas">
        <item name="android:layout_width">fill_parent</item>
     	<item name="android:layout_height">wrap_content</item>
		<item name="android:ellipsize">end</item>
        <item name="android:textColor">#fff</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    
    <style name="SegundaLineaListas">
        <item name="android:layout_width">fill_parent</item>
     	<item name="android:layout_height">wrap_content</item>
		<item name="android:ellipsize">end</item>
        <item name="android:textColor">#e7e7e7</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">normal</item>
    </style>
	    
        
    <style name="HeadGrilla">
        <item name="android:background">#3AA4F0</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="TextoHeadGrilla">
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:shadowRadius">1.0</item>
        <item name="android:padding">5dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:typeface">serif</item>
    </style>

    <style name="BodyGrilla">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">25dp</item>
    </style>

    <style name="TextoBodyGrilla">
        <item name="android:textColor">#2A58A5</item>
        <item name="android:shadowRadius">1.0</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">15sp</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:singleLine">true</item>
    </style>
    
     <style name="TextoBodyGrilla2">
        <item name="android:textColor">#2A58A5</item>
        <item name="android:shadowRadius">1.0</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">15sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
    </style>
    
        
    <style name="TextoLabel">
        <item name="android:textColor">#ffffff</item>
	    <item name="android:shadowRadius">1.0</item>
    	<item name="android:gravity">center</item>
     	<item name="android:textSize">18sp</item>
     	<item name="android:layout_width">wrap_content</item>
     	<item name="android:layout_height">wrap_content</item>
     	<item name="android:textStyle">bold</item>
    </style>
    
    <style name="Lista">
        <item name="android:layout_width">fill_parent</item>
     	<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_marginTop">10dp</item>
		<item name="android:paddingLeft">10dp</item>
		<item name="android:paddingRight">10dp</item>
		<item name="android:cacheColorHint">#00000000</item>
    </style>
    
     <style name="BtnGuardar">
        <item name="android:layout_width">45dp</item>
     	<item name="android:layout_height">45dp</item>
		<item name="android:layout_gravity">right</item>
		<item name="android:layout_marginLeft">15dp</item>
		<item name="android:background">@drawable/guadar</item>
		<item name="android:text"></item>
    </style>

	<style name="SubtituloTwo">
<!--         ef0094 -->
       <item name="android:textColor">#ffffff</item>
        <item name="android:typeface">sans</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:singleLine">false</item>
        <item name="android:maxLines">4</item>
    </style>         

</resources>