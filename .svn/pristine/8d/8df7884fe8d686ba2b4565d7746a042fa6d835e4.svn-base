<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/tr_reporte_revestimiento_presmat_sanfernando_aavv"
	style="@style/BodyGrilla" >

    <TextView
        android:id="@+id/tv_sku"
        style="@style/TextoBodyGrilla"
        android:width="60dp" />

   
        <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center_vertical|center_horizontal"
        android:paddingBottom="5dp"
        android:paddingTop="5dp" >

            <EditText
            android:id="@+id/et_cantidad"
        style="@style/TextoBodyGrilla2"
        android:singleLine="true"
            android:layout_width="80dp"
            android:inputType="number"
             />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center_vertical|center_horizontal"
        android:paddingBottom="5dp"
        android:paddingTop="5dp" >

        <CheckBox
            android:id="@+id/ck_foto"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:button="@drawable/checkboxstyle" />
    </LinearLayout>

</TableRow>