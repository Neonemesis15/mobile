<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/tr_reporte_precio_alicorp_autoservicio"
    style="@style/BodyGrilla" >

    <TextView
        android:id="@+id/tv_sku"
        style="@style/TextoBodyGrilla2"
         android:width="75dp"
    />

    <EditText
        android:id="@+id/et_ppdv"
        style="@style/TextoBodyGrilla2"
        android:singleLine="true"
         android:numeric="decimal"
         android:width="100dp"
        />

    <EditText
        android:id="@+id/et_poferta"
        style="@style/TextoBodyGrilla2"
        android:singleLine="true"
        android:numeric="decimal"
         android:width="90dp"
        />
    
    <Spinner
	    android:id="@+id/spinnerMObs"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:prompt="@string/mobs_prompt"
	     android:width="130dp"
	    />
</TableRow>